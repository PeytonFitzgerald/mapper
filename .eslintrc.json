{
    "extends": [ "plugin:prettier/recommended"],
    "parser": "@typescript-eslint/parser",
    "rules": {
      "prettier/prettier": "error",
      "no-unused-vars": "warn",
      "no-console": "off",
      "func-names": "off",
      "no-process-exit": "off",
      "object-shorthand": "off",
      "class-methods-use-this": "off"
    }
  }